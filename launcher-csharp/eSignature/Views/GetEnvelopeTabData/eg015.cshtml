<h4>@Html.Raw(ViewBag.CodeExampleText.ExampleName)</h4>

@Html.Raw(ViewBag.CodeExampleText.ExampleDescription)

@if (ViewBag.showDoc) {
<p><a target='_blank' href='@ViewBag.documentation'>Documentation</a> about this example</p>
}

<partial name="../../../Views/Shared/LinkToMethodView" model="ViewBag.CodeExampleText" /> 

<p>
    View source file <a target="_blank" href="@ViewBag.source">GetEnvelopeTabData.cs</a> on GitHub.
</p>

@if (ViewBag.envelopeOk) {
<p>
    The last envelope you created with this example launcher will be queried.
    Recommendation: Use Send an envelope using a template, then this example, since Send an envelope using a template includes many tabs of different types.
</p>

<form class="eg" action="" method="post" data-busy="form">
    <input type="hidden" name="_csrf" value="<%- csrfToken %>">
    <button type="submit" class="btn btn-primary">Continue</button>
</form>
} else {
<p>
    Prerequisite: Please first create an envelope using <a href="eg009">Send an envelope using a template</a>.
</p>

<form class="eg" action="eg009" method="get">
    <button type="submit" class="btn btn-primary">Continue</button>
</form>

}
